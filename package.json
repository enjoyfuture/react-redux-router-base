{
  "name": "node-react-redux-base-demo",
  "version": "0.1.0",
  "author": "wangyanjun",
  "description": "node-react-redux-base-demo",
  "keywords": [
    "react",
    "router",
    "redux",
    "immutable",
    "webpack"
  ],
  "scripts": {
    "clean": "rimraf ./public/dist",
    "dll": "webpack --progress --config webpack.config.dll.babel.js",
    "generateEnv": "node ./setup-env.js",
    "ie8": "sh ./ie8/rmJsnext.sh && node ./ie8/support-ie8.js",
    "es3ify": "node ./ie8/es3ify.js",
    "prebuild": "npm run clean && npm run generateEnv && npm run ie8",
    "build": "webpack --progress --colors --config webpack.build.babel.js && npm run es3ify",
    "postbuild": "sh ./to-dist.sh",
    "prestart": "cross-env NODE_ENV=development npm run generateEnv && npm run clean",
    "start": "nodemon ./bin/www",
    "startProd": "node ./dist/bin/www",
    "test": "nyc node node_modules/.bin/ava",
    "test-watch": "npm run test -- --watch",
    "check-coverage": "nyc check-coverage --statements 100 --branches 100 --functions 100 --lines 100"
  },
  "dependencies": {
    "body-parser": "^1.15.2",
    "classnames": "^2.2.5",
    "compression": "^1.6.2",
    "cookie-parser": "^1.4.3",
    "es5-shim": "^4.5.9",
    "es6-promise": "^4.0.5",
    "express": "^4.14.0",
    "fetch-ie8": "^1.4.3",
    "handlebars": "^4.0.6",
    "hbs": "^4.0.1",
    "html5shiv": "^3.7.3",
    "immutable": "^3.8.1",
    "ip": "^1.1.4",
    "isomorphic-fetch": "^2.2.1",
    "moment": "^2.17.1",
    "normalize.css": "^5.0.0",
    "react": "^15.4.2",
    "react-addons-css-transition-group": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.1",
    "react-router-redux-fixed": "^4.0.8",
    "redux": "^3.6.0",
    "redux-immutable": "^3.0.10",
    "redux-thunk": "^2.1.0",
    "request": "^2.79.0",
    "respond.js": "^1.4.2",
    "serialize-javascript": "^1.3.0",
    "winston": "^2.3.0",
    "winston-daily-rotate-file": "^1.4.2"
  },
  "devDependencies": {
    "autoprefixer": "^6.6.1",
    "ava": "^0.17.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es3-member-expression-literals": "^6.8.0",
    "babel-plugin-transform-es3-property-literals": "^6.8.0",
    "babel-plugin-transform-flow-comments": "^6.21.0",
    "babel-plugin-transform-flow-strip-types": "^6.21.0",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-register": "^6.18.0",
    "cross-env": "^3.1.4",
    "css-loader": "^0.26.1",
    "enzyme": "^2.7.0",
    "es3ify": "^0.2.2",
    "eslint": "^3.13.1",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-ava": "^4.0.1",
    "eslint-plugin-flowtype": "^2.30.0",
    "eslint-plugin-react": "^6.9.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "file-loader": "^0.9.0",
    "immutable": "^3.8.1",
    "jsdom": "^9.9.1",
    "less": "^2.7.2",
    "less-loader": "^2.2.3",
    "mock-css-modules": "^1.0.0",
    "nock": "^9.0.2",
    "nodemon": "^1.11.0",
    "nyc": "^10.0.0",
    "postcss-loader": "^1.2.2",
    "precss": "^1.4.0",
    "react-addons-perf": "^15.4.2",
    "react-addons-test-utils": "^15.4.2",
    "redux-devtools": "^3.3.2",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.2.0",
    "redux-logger": "^2.7.4",
    "rimraf": "^2.5.4",
    "sinon": "^1.17.7",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.0-rc.6",
    "webpack-dev-middleware": "^1.9.0",
    "webpack-hot-middleware": "^2.15.0",
    "webpack-mapping-plugin": "^0.1.1"
  },
  "engines": {
    "node": ">=6.9.1"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.cbpmgt.com:fe-team/node-react-redux-base-demo.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "http://gitlab.cbpmgt.com/fe-team/node-react-redux-base-demo/issues"
  },
  "homepage": "http://gitlab.cbpmgt.com/fe-team/node-react-redux-base-demo#readme",
  "ava": {
    "files": [
      "client/**/*.spec.js"
    ],
    "source": [
      "client/**/*.js"
    ],
    "concurrency": 5,
    "failFast": true,
    "tap": true,
    "babel": "inherit",
    "require": [
      "./setup-test-env.js"
    ]
  },
  "nyc": {
    "include": [
      "client/**/*.js"
    ],
    "exclude": [
      "client/**/*.spec.js"
    ],
    "reporter": [
      "lcov",
      "text",
      "html"
    ],
    "cache": true
  }
}
